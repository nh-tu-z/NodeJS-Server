<body>
<!-- main -->
	<div class="sub-main-w3"> 
		<div class="wthree-pro">
			<h2>Sign Up</h2>
		</div>
		<form action="/site/sign-up-form" method="post">
			<div class="pom-agile">
				<input placeholder="Username" name="Name" class="user" type="text" required="">
			</div>
			<div class="pom-agile">
				<input  placeholder="Your Email Adrress" name="email" class="pass" type="email" required="">
			</div>
			<div class="pom-agile">
				<input  placeholder="Your Phone Number" name="Phone" class="pass" type="tel" pattern="[0]{1}[0-9]{9}" required="">
			</div>
			<div class="pom-agile">
				<input  placeholder="Password" name="Password" class="pass" type="password" required="">
			</div>
			<div class="pom-agile">
				<input  placeholder="Confirmed Password" name="Password" class="pass" type="password" required="">
			</div>
			<div class="sub-w3l">
				<div class="right-w3l">
					<a class="link-sign-up" href="./login-form">Sign in</a>
					<input type="submit" value="Register">
				</div>
			</div>
		</form>
		{{#each message}}
			<div class="right-w3l alert alert-danger" role="alert">
				{{this}}
			</div>
		{{/each}}
	</div>
<!--//main-->
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
